<%= form_with(model: stock, data: { turbo: false }) do |form|%>
  
	<div>
		<% stock.errors.full_messages.each do |message| %>
      <div><%= message %></div>
    <% end %>
	</div>
  
	<div class="form-outline mb-4">
		<%= form.label :companyname%><br>
		<%= form.text_field :companyname, class: "form-control"%>	
	</div>

	<div class="form-outline mb-4">
		<%= form.label :stockprice%><br>
		<%= form.number_field :stockprice, class: "form-control"%>	
	</div>

	<div>
		<%= form.submit class: 'btn btn-primary' %>
	</div>

<%end%>
