
<div class="card text-center m-5 border border-dark">
  <div class="card-header">
    <h3><%= link_to @stock.companyname, stock_transactions_path(@stock) %></h3>
  </div>
  <div class="card-body">
    <p class="card-text"><%= @stock.stockprice %> stock price</p>
    <div class="  d-flex justify-content-around ">
    <%= link_to 'transactions',stock_transactions_path(@stock), class: 'btn btn-primary'%>
    <%= back_link(stocks_path) %>
  </div>
  </div>
</div>






